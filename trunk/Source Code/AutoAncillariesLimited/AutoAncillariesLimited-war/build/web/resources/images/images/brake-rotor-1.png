    <script type="text/javascript">
        $(document).ready(function() {
            $(".txtNewsEmailHome").live("keydown", function(event) {
                if (event.keyCode == 13) {
                    submitPage = false;
                }
            });

            $("a.iframe.arrows").fancybox({
                'transitionIn': 'none',
                'transitionOut': 'none',
                'speedIn': 100,
                'speedOut': 100,
                'modal': false,
                'hideOnContentClick': false,
                'hideOnOverlayClick': true,
                'showCloseButton': true,
                'scrolling': 'no',
                'width': 760,
                'height': 390,
                'padding': 0,
                'onComplete': iframeLoad,
                'margin': 0
            });

            $(".slider-number").each(function(index) {
                $(this).click(function() {
                    $("#slider-play-toggle").attr("play", "false");
                    $(".slider-image").css("z-index", "0");
                    $(".slider-image.active").css("z-index", "1");
                    $(".slider-image").eq(index).css("z-index", "2");
                    $(".slider-number").removeClass("active");
                    $(this).addClass("active");
                    $(".slider-image").addClass("hidden");
                    $(".slider-image").eq(index).fadeIn(300, function() {
                        $(".slider-image").eq(index).removeClass("hidden");
                        $(".slider-image.hidden").hide();
                    });
                });
            });

            $("#slider-play-toggle").live('click', function() {
					var $toggle = $("#slider-play-toggle span").removeClass('ni-pause ni-play');
                if ($(this).attr("play") == "true") {
                    $(this).attr("play", "false");
                    $toggle.addClass('ni-play');
                } else {
                    $(this).attr("play", "true");
                    $toggle.addClass('ni-pause');
                }
            });

            playSlider();
        });

        function playSlider() {
            window.setInterval(function() {
                if ($("#slider-play-toggle").attr("play") == "true") {
                    if ($(".slider-number.active").next(".slider-number").length == 0) {
                        $(".slider-image").css("z-index", "0");
                        $(".slider-image.active").css("z-index", "1");
                        $(".slider-image").first().css("z-index", "2");
                        $(".slider-number").removeClass("active");
                        $(".slider-number").first().addClass("active");
                        $(".slider-image").addClass("hidden");
                        $(".slider-image").first().fadeIn(1000, function() {
                            $(".slider-image").first().removeClass("hidden");
                            $(".slider-image.hidden").hide();
                        });
                    } else {
                        var index = $(".slider-number.active").next(".slider-number").index(".slider-number");                        
                        $(".slider-image").css("z-index", "0");
                        $(".slider-image.active").css("z-index", "1");
                        $(".slider-image").eq(index).css("z-index", "2");
                        $(".slider-number").removeClass("active");
                        $(".slider-number").eq(index).addClass("active");
                        $(".slider-image").addClass("hidden");
                        $(".slider-image").eq(index).fadeIn(1000, function() {
                            $(".slider-image").eq(index).removeClass("hidden");
                            $(".slider-image.hidden").hide();
                        });
                    }
                }
            }, 5000);
        }
    
	</script>

